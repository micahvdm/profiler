@prefix modgui: <http://moddevices.com/ns/modgui#> .
@prefix lv2:    <http://lv2plug.in/ns/lv2core#> .

<https://faustlv2.bitbucket.io/kpp_tubeamp>
    modgui:gui [
        modgui:resourcesDirectory <modgui> ;
        modgui:iconTemplate <modgui/icon-kpptubeamp.html> ;
        modgui:stylesheet <modgui/stylesheet-kpptubeamp.css> ;
        modgui:screenshot <modgui/screenshot-kpptubeamp.png> ;
